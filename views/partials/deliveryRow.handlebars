<tr class="order-row" data-id="{{id}}" data-d-id="{{driver_id}}" data-t-id="{{truck_id}}" data-sf-id="{{start_facility_id}}" data-ef-id="{{end_facility_id}}" data-weight="{{total_weight}}" data-volume="{{total_volume}}" data-late="{{late}}" data-dt="{{departure_time}}" data-eat="{{expected_arrival_time}}" data-aat="{{actual_arrival_time}}">
  <th>add</th>
  <th>delete</th>
  <th>{{id}}</th>
  <th>{{driver_id}}</th>
  <th>{{truck_id}}</th>
  <th>{{start_facility_id}}</th>
  <th>{{end_facility_id}}</th>
  <th>{{total_weight}}</th>
  <th>{{total_volume}}</th>
  <th>{{late}}</th>
  <th>{{departure_time}}</th>
  <th>{{expected_arrival_time}}</th>
  <th>{{actual_arrival_time}}</th>
